<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.dao.HuanpiaoDao">

	<!-- 可根据自己的需求，是否要使用 -->
    <resultMap type="com.entity.HuanpiaoEntity" id="huanpiaoMap">
        <result property="dingdanbianhao" column="dingdanbianhao"/>
        <result property="yonghuming" column="yonghuming"/>
        <result property="xingming" column="xingming"/>
        <result property="shenfenzhenghao" column="shenfenzhenghao"/>
        <result property="shoujihao" column="shoujihao"/>
        <result property="huanpiaomudedi" column="huanpiaomudedi"/>
        <result property="sfsh" column="sfsh"/>
        <result property="shhf" column="shhf"/>
    </resultMap>

	<select id="selectListVO"
		resultType="com.entity.vo.HuanpiaoVO" >
		SELECT * FROM huanpiao  huanpiao         
        <where> 1=1 ${ew.sqlSegment}</where>
	</select>
	
	<select id="selectVO"
		resultType="com.entity.vo.HuanpiaoVO" >
		SELECT  huanpiao.* FROM huanpiao  huanpiao 	
 		<where> 1=1 ${ew.sqlSegment}</where>
	</select>

    <select id="selectListView"
		resultType="com.entity.view.HuanpiaoView" >

		SELECT  huanpiao.* FROM huanpiao  huanpiao 	        
        <where> 1=1 ${ew.sqlSegment}</where>
	</select>
	
	<select id="selectView"
		resultType="com.entity.view.HuanpiaoView" >
		SELECT * FROM huanpiao  huanpiao <where> 1=1 ${ew.sqlSegment}</where>
	</select>
	
</mapper>